<h1>Log in</h1>

<p>Entre tes infos</p><br>

<%= form_for(:session, url: login_path) do |f| %>

  <%= label_tag 'Email:' %>
  <%= f.text_field :email, placeholder: "michou@gmail.com"%><br>

  <%= label_tag 'Mot de passe :' %>
  <%= f.text_field :password %><br>

  <%= f.submit "Je me connecte !" %>

<% end %>

